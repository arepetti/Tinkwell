{
  "name": "tinkwell-syntax",
  "displayName": "Tinkwell configurations Syntax Highlighting",
  "description": "Syntax highlighting for Tinkwell configuration files",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": ["Programming Languages"],
  "contributes": {
    "languages": [
      {
        "id": "tinkwell_ensamble",
        "aliases": ["TinkWell_Ensamble"],
        "extensions": [".ensamble", ".tw"],
        "configuration": "./language-configuration.json"
      },
      {
        "id": "tinkwell_reducer",
        "aliases": ["TinkWell_Reducer"],
        "extensions": [".twm"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tinkwell_ensamble",
        "scopeName": "source.tw",
        "path": "./syntaxes/tw.tmLanguage.json"
      },
      {
        "language": "tinkwell_reducer",
        "scopeName": "source.twm",
        "path": "./syntaxes/twm.tmLanguage.json"
      }
    ]
  }
}