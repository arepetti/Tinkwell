{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "Endpoints": {
      "Http2": {
        "Url": "https://localhost:5000"
      }
    },
    "EndpointDefaults": {
      "Protocols": "Http2"
    }
  },
  "Supervisor": {
    "CommandServer": {
      "Enabled": true,
      "PipeName": "tinkwell-supervisor-command-server",
      "MaxConcurrentConnections": 4
    }
  },
  "Discovery": {
    "Master": ""
  }
}
